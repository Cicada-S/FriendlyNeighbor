/* pages/community/community.wxss */
.container {
  width: 100vw;
}
.container .search {
  position: fixed;
  width: 100%;
}
.container .communityList {
  padding: 110rpx 30rpx 0;
}
.container .communityList .communityItem {
  display: flex;
  padding: 20rpx;
}
.container .communityList .communityItem .image {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.container .communityList .communityItem .content {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin-left: 20rpx;
}
.container .communityList .communityItem .content .city {
  color: var(--Color-FontA);
  font-size: 28rpx;
}
.container .overlau {
  box-sizing: content-box;
  position: fixed;
  bottom: 0;
  width: 100%;
  padding: 10rpx 30rpx;
  background-color: var(--Color-White);
}
.container .overlau .bottomBtn {
  width: 690rpx;
  height: 80rpx;
  color: var(--Color-White);
  font-size: 30rpx;
  text-align: center;
  line-height: 80rpx;
  letter-spacing: 10rpx;
  border-radius: 10rpx;
  background-color: var(--Color-MainA);
  box-shadow: 2rpx 4rpx 12rpx 2rpx rgba(64, 126, 245, 0.8);
}
