/* pages/myCommunity/myCommunity.wxss */
.container {
  width: 100vw;
  padding: 20rpx 30rpx 0;
}
.container .communityInfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container .communityInfo .qrCode {
  width: 200rpx;
  height: 200rpx;
  background-color: var(--Color-MainA);
}
.container .communityInfo .name {
  font-size: 36rpx;
  font-weight: 550;
  margin: 20rpx 0;
}
.container .change {
  width: 100%;
  color: var(--Color-White);
  letter-spacing: 10rpx;
  margin-top: 30rpx;
  background-color: var(--Color-MainA);
}
