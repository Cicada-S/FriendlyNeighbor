/* pages/follow/follow.wxss */
.container {
  width: 100vw;
}
.container .communityList {
  padding: 0 30rpx;
}
.container .communityList .communityItem {
  display: flex;
  align-items: center;
  padding: 20rpx;
}
.container .communityList .communityItem .content {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.container .communityList .communityItem .content .city {
  color: var(--Color-FontA);
  font-size: 28rpx;
}
.container .communityList .communityItem .joinBtn {
  width: 100rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: var(--Color-White);
  border-radius: 10rpx;
  background-color: var(--Color-ViceA);
}
