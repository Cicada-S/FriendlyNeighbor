/* components/Search/Search.wxss */
.search {
  height: 70rpx;
  margin: 20rpx;
  padding: 0 40rpx;
  font-size: 32rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-radius: 50rpx;
  background-color: #FFFFFF;
  box-shadow: inset 5px 5px 10px #ededed, inset -5px -5px 10px #ffffff;
}
.search .input {
  width: 100%;
  height: 100%;
  line-height: 84rpx;
  outline: none;
}
